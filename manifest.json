{
    "applicationName": "Network deployment",
    "contactEmail": "gianni@ebi.ac.uk",
    "about": "Deploy a private network",
    "version": "0.5",
    "cloudProviders": [{
            "cloudProvider": "AWS",
            "path": "aws"
        },
        {
            "cloudProvider": "GCP",
            "path": "gcp",
            "inputs": ["project", "region"]
        },
        {
            "cloudProvider": "OSTACK",
            "path": "ostack"
        },
        {
            "cloudProvider": "AZURE",
            "path": "azure",
	    "inputs": ["region","client_id","client_secret","tenant_id","subscription_id"]
        }
    ],
    "inputs": ["network_name"],
    "outputs": ["network_id", "network_name", "subnet_id"]
}
